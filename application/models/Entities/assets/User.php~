<?php

namespace Entities\assets;

use Doctrine\ORM\Mapping as ORM;

/**
 * Entities\assets\User
 */
class User
{
    /**
     * @var integer $idUser
     */
    private $idUser;

    /**
     * @var string $codeDept 
     */
    private $codeDept;

    /**
     * @var integer $staffUser
     */
    private $staffUser;

    /**
     * @var string $nomPrenomUser
     */
    private $nomPrenomUser;

    /**
     * @var string $fonction
     */
    private $fonction;

    /**
     * @var string $emailUser
     */
    private $emailUser;

    /**
     * @var string $login
     */
    private $login;

    /**
     * @var string $mdp
     */
    private $mdp;


    /**
     * Get idUser
     *
     * @return integer 
     */
    public function getIdUser()
    {
        return $this->idUser;
    }

    /**
     * Set codeDept
     *
     * @param string $codeDept
     * @return User
     */
    public function setCodeDept($codeDept)
    {
        $this->codeDept = $codeDept;
        return $this;
    }

    /**
     * Get codeDept
     *
     * @return string 
     */
    public function getCodeDept()
    {
        return $this->codeDept;
    }

    /**
     * Set staffUser
     *
     * @param integer $staffUser
     * @return User
     */
    public function setStaffUser($staffUser)
    {
        $this->staffUser = $staffUser;
        return $this;
    }

    /**
     * Get staffUser
     *
     * @return integer 
     */
    public function getStaffUser()
    {
        return $this->staffUser;
    }

    /**
     * Set nomPrenomUser
     *
     * @param string $nomPrenomUser
     * @return User
     */
    public function setNomPrenomUser($nomPrenomUser)
    {
        $this->nomPrenomUser = $nomPrenomUser;
        return $this;
    }

    /**
     * Get nomPrenomUser
     *
     * @return string 
     */
    public function getNomPrenomUser()
    {
        return $this->nomPrenomUser;
    }

    /**
     * Set fonction
     *
     * @param string $fonction
     * @return User
     */
    public function setFonction($fonction)
    {
        $this->fonction = $fonction;
        return $this;
    }

    /**
     * Get fonction
     *
     * @return string 
     */
    public function getFonction()
    {
        return $this->fonction;
    }

    /**
     * Set emailUser
     *
     * @param string $emailUser
     * @return User
     */
    public function setEmailUser($emailUser)
    {
        $this->emailUser = $emailUser;
        return $this;
    }

    /**
     * Get emailUser
     *
     * @return string 
     */
    public function getEmailUser()
    {
        return $this->emailUser;
    }

    /**
     * Set login
     *
     * @param string $login
     * @return User
     */
    public function setLogin($login)
    {
        $this->login = $login;
        return $this;
    }

    /**
     * Get login
     *
     * @return string 
     */
    public function getLogin()
    {
        return $this->login;
    }

    /**
     * Set mdp
     *
     * @param string $mdp
     * @return User
     */
    public function setMdp($mdp)
    {
        $this->mdp = $mdp;
        return $this;
    }

    /**
     * Get mdp
     *
     * @return string 
     */
    public function getMdp()
    {
        return $this->mdp;
    }
}